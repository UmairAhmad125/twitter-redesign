
<div class="d-flex flex-row no-hover">
    <div class="">
        <%= render 'shared/left-nav'%>
    </div>
    <main class="d-flex flex-column main">
        <div>
            <%= render 'shared/top-nav'%>
        </div>
        <div class="d-flex flex-column">
            <% if !check_photo(current_user) %>
                <div class=d-flex flex-column>
                    <p class="text-primary ">Complete your profile</p>
                    <%= link_to 'click here', edit_user_registration_path, class: "text-danger ml-1" %>
                </div>
            <% end %>
            <div class="main-content d-flex flex-row background-white all-users">
                <div class="d-flex flex-column ">
                    <div class="feed-container p-2 ml-4 background-white">
                        <% if @user.count > 0 %>
                            <% @user.each do |u| %>
                                <div class="d-flex flex-row mt-2 align-items-center">
                                    <% if check_photo(u) %>
                                        <%= image_tag url_for(u.Photo) %>                     
                                    <% else %>
                                        <img class="profile-pic" src="data:image/jpeg;base64," alt="profile">
                                    <% end %>
                                    <div class="follow-all d-flex flex-column align-items-center justify-content-center">
                                        <h5 class="mt-3 ml-2">
                                            <%= link_to check_user_name(u) ,  users_show_path('id' => u.id) %>
                                        </h5>
                                        <h4>
                                        <%= check_follower(u) %>
                                        </h4>
                                    </div>
                                </div>  
                            <% end %>                  
                        <% end %>
                    </div>
                </div>
                <div class="d-flex flex-column fixed-right align-items-center background-off p-2">
                    <% if check_photo(current_user) %>
                        <%= image_tag url_for(current_user.Photo) %>                     
                    <% else %>
                        <img class="profile-pic" src="data:image/jpeg;base64," alt="profile">
                    <% end %>
                    <h5 class="mt-2">
                        <%= link_to check_user_name(current_user) ,  users_show_path('id' => current_user.id)%>
                    </h5>
                    <div class="d-flex flex-row mt-2">
                        <div class="d-flex flex-column align-items-center  following p-1">
                            <p>
                                <%= current_user.followed.count %>
                            </p>
                            <h5 class="font-12">Following</h5>
                        </div>
                        <div class="d-flex flex-column align-items-center followers p-1">
                            <p>
                                <%= current_user.followers.count %>
                            </p>
                            <h5 class="font-12">Followers</h5>`
                        </div>
                        <div class="d-flex flex-column align-items-center followers p-1">
                            <p>
                                <%= current_user.opinions.count %>
                            </p>
                            <h5 class="font-12">Twitchs</h5>`
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </main>
</div>